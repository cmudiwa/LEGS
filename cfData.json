    {
      "DisplayOnView": true,
      "Key": "fc6f05f9-50c7-46c0-b9f5-7fe0ca83cf2a",
      "Description": "<div id='cfActions'><script>\ndocument.addEventListener('DOMContentLoaded', function () {\n    const customFields = [\n\t\t{ label: 'Payment Type 1', actions: ['clear'] },\n\t\t{ label: 'Payment Amount 1', actions: ['clear'] },\n\t\t{ label: 'Payment Type 2', actions: ['clear'] },\n\t\t{ label: 'Payment Amount 2', actions: ['clear'] },\n\t\t{ label: 'Receipt Date', actions: ['readonly', 'clear'] },\n\t\t{ label: 'DeviceID', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'DeviceSN', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'FiscalDayNo', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'Receipt GlobalNo', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'Receipt Counter', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'Receipt NoRef', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'Receipt Notes', actions: ['clear'] },\n\t\t{ label: 'Receipt DateRef', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'DeviceID Ref', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'DeviceSN Ref', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'FiscalDayNo Ref', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'Receipt CounterRef', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'Receipt GlobalNoRef', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'Approval Status', actions: ['readonly', 'clear'] },\n\t\t{ label: 'Receipt QRCode', actions: ['readonly', 'clear'] },\n\t\t{ label: 'Verification Code', actions: ['readonly', 'hide', 'clear'] },\n\t\t{ label: 'Server Response', actions: ['readonly', 'clear'] },\n\t];\n\n    const isUpdate = document.querySelector('button.btn.btn-success[onclick=\"ajaxPost(true)\"]') !== null;\n\n    if (!isUpdate) {\n\t\tif (app && app.CustomFields2) {\n\t\t\tif (app.CustomFields2.Strings) {\n\t\t\t\tfor (let guid in app.CustomFields2.Strings) {\n\t\t\t\t\tif (app.CustomFields2.Strings.hasOwnProperty(guid)) {\n\t\t\t\t\t\tdelete app.CustomFields2.Strings[guid];\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t}\n\t\t\tif (app.CustomFields2.Decimals) {\n\t\t\t\tfor (let guid in app.CustomFields2.Decimals) {\n\t\t\t\t\tif (app.CustomFields2.Decimals.hasOwnProperty(guid)) {\n\t\t\t\t\t\tdelete app.CustomFields2.Decimals[guid];\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}\n\n    const disableDateAndReference = () => {\n        const dateInput = document.querySelector('.mx-input');\n        if (dateInput) {\n            dateInput.disabled = true;\n            const calendarIcon = document.querySelector('.mx-icon-calendar');\n            if (calendarIcon) {\n                calendarIcon.style.pointerEvents = 'none';\n                calendarIcon.style.opacity = '0.5';\n            }\n        }\n\n        const checkbox = document.querySelector('.input-group-addon input[type=\"checkbox\"]');\n        const inputText = document.querySelector('.input-group input[type=\"text\"]');\n        if (checkbox && inputText) {\n            try {\n                if (typeof app !== 'undefined') {\n                    app.AutomaticReference = false;\n                }\n                checkbox.checked = false;\n                checkbox.disabled = true;\n                inputText.disabled = true;\n            } catch (error) {}\n        }\n    };\n\n    const processCustomFields = () => {\n        const customFieldsFieldset = [...document.querySelectorAll('fieldset')]\n            .find(fieldset => fieldset.querySelector('legend')?.textContent.trim() === 'Custom Fields');\n        \n        if (!customFieldsFieldset) return;\n\n        customFields.forEach(field => {\n            let customField = null;\n            customField = Array.from(customFieldsFieldset.querySelectorAll('.form-group'))\n                .find(group => group.querySelector('label')?.textContent.trim() === field.label);\n\n            if (!customField) return;\n\n            const input = customField.querySelector('input, textarea');\n            const dropdown = customField.querySelector('.vs__dropdown-toggle');\n\n            if (field.actions.includes('hide')) {\n                customField.style.display = 'none';\n            }\n\n            if (field.actions.includes('readonly')) {\n                if (input) {\n                    input.setAttribute('readonly', 'true');\n                }\n                if (dropdown) {\n                    const searchInput = dropdown.querySelector('input.vs__search');\n                    const clearButton = dropdown.querySelector('.vs__clear');\n                    dropdown.setAttribute('disabled', 'true');\n                    if (searchInput) searchInput.setAttribute('readonly', 'true');\n                    if (clearButton) clearButton.style.display = 'none';\n                    dropdown.style.pointerEvents = 'none';\n                }\n            }\n\n            if (!isUpdate && field.actions.includes('clear')) {\n                if (input) {\n                    input.value = '';\n                }\n                if (dropdown) {\n                    const searchInput = dropdown.querySelector('input.vs__search');\n                    if (searchInput) searchInput.value = '';\n                }\n            }\n        });\n    };\n\n    const disableUpdateButtonIfApproved = () => {\n        const approvalField = customFields.find(field => field.label === 'Approval Status');\n        if (!approvalField) return;\n\n        const customFieldsFieldset = [...document.querySelectorAll('fieldset')]\n            .find(fieldset => fieldset.querySelector('legend')?.textContent.trim() === 'Custom Fields');\n        \n        if (!customFieldsFieldset) return;\n\n        const customField = Array.from(customFieldsFieldset.querySelectorAll('.form-group'))\n            .find(group => group.querySelector('label')?.textContent.trim() === 'Approval Status');\n\n        if (!customField) return;\n\n        const input = customField.querySelector('input, textarea');\n\n        if (input && input.value.trim() === 'APPROVED' && isUpdate) {\n            const updateButton = document.querySelector('button.btn.btn-success[onclick=\"ajaxPost(true)\"]');\n            if (updateButton) {\n                updateButton.disabled = true;\n            }\n        }\n    };\n\n    try {\n        disableDateAndReference();\n        processCustomFields();\n        disableUpdateButtonIfApproved();\n    } catch (error) {}\n\n    const selfDeletingContainer = document.getElementById('cfActions'); \n    if (selfDeletingContainer) { \n        selfDeletingContainer.remove();\n    }\n});\n</script></div>",
